void setup() {
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_I2C_ADDR);
  display.setTextColor(WHITE);
  display.clearDisplay();
}

int frame = 0;
void loop() {
  breathing();
}

void breathing() {
  display.fillRect(40, 8, 48, 48, BLACK);
  display.setCursor(41, 50);
  display.println("breathe.");
  display.drawBitmap(40, 8, breathAni[frame], FRAME_WIDTH[0], FRAME_HEIGHT[0], WHITE);
  frame = (frame + 1) % BREATH_COUNT;
  display.display();
  delay(FRAME_DELAY);
}

void listing() {
  display.fillRect(80, 15, 48, 48, BLACK);
  display.drawBitmap(80, 15, listAni[frame], FRAME_WIDTH[1], FRAME_HEIGHT[1], WHITE);
  frame = (frame + 1) % BREATH_COUNT;
  display.display();
  delay(FRAME_DELAY);
}

void checklist() {
  display.setCursor(8, 5);
  display.println("Self Care Checklist:");
  display.display();
}
